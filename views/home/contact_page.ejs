<% include ../partials/header %>
	<div id="page">
		<nav class="colorlib-nav" role="navigation">
			<div class="top-menu">
				<div class="container">
					<div class="row">
						<div class="col-xs-2">
                            <div id="colorlib-logo"><a href="/">Unity cars</a></div>
                        </div>
                        <div class="col-xs-10 text-right menu-1">
                            <ul class="captial">
                                <%if(currentUser){%>
                                    <li ><a href="/admin/view_all_cars">Admin</a></li>
                                <%}%>
                                <li ><a href="/">Home</a></li>
                                <li class="active"><a href="/buy-cars">Buy Cars</a></li>
                                <li><a href="/sell-cars">Sell Cars</a></li>
                                <li><a href="/contact-us">Get In Touch</a></li>
                                <%if(currentUser){%>
                                    <li><a href="/index/logout">Logout</a></li>
                                <%}%>
                            </ul>
                        </div>
					</div>
				</div>
			</div>
		</nav>
		<aside id="colorlib-hero" class="breadcrumbs">
			<div class="flexslider">
				<ul class="slides">
			   	<li style="background-image: url(/images/cover-img-1.jpg);">
			   		<div class="overlay"></div>
			   		<div class="container-fluid">
			   			<div class="row">
				   			<div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
				   				<div class="slider-text-inner text-center">
				   					<h1>Fill form to contact to our team.</h1>
				   					
				   				</div>
				   			</div>
				   		</div>
			   		</div>
			   	</li>
			  	</ul>
		  	</div>
		</aside>

		<div class="colorlib-shop">
			<div class="container">
				
				<div class="row">
					<div class="col-md-10 col-md-offset-1 text-center">

                            <div id="contact_form_hide" class="card card-2">
								<form style="padding:1em;" id="querie_form">

									<div class="form-row">
										<div class="form-group col-md-6">
											<div class="form-row">
												<div class="form-group">
													<label for="name">Name</label>
													<input type="text" name="name" class="form-control testimonnial_input" id="nameQ" placeholder="Name">
												</div>
											</div>
										</div>
										<div class="form-group col-md-6">
											<div class="form-row">
												<div class="form-group">
													<label for="email">Email Address</label>
													<input type="text" name="email" class="form-control testimonnial_input" id="emailQ" placeholder="Email Address">
												</div>
											</div>
										</div>
									</div>

									<div class="form-row">
										<div class="form-group col-md-6">
											<div class="form-row">
												<div class="form-group">
													<label for="phone">Phone No.</label>
													<input type="text" name="phone" class="form-control testimonnial_input" id="phoneQ" placeholder="Phone Number">
												</div>
											</div>
										</div>
										<div class="form-group col-md-6">
											<div class="form-row">
												<div class="form-group">
													<label for="address">Full Address</label>
													<input type="text" name="address" class="form-control testimonnial_input" id="addressQ" placeholder="Full Address">
												</div>
											</div>
										</div>
									</div>

									<div class="form-row">
										<div class="form-group col-md-6">
											<div class="form-row">
												<div class="form-group">
													<input type="hidden" name="carName" class="form-control testimonnial_input" id="carNameQ" value="<%=data%>">
												</div>
											</div>
										</div>
										<div class="form-group col-md-6">
											<div class="form-row">
												<div class="form-group">
													<input type="hidden" name="purpose" class="form-control testimonnial_input" id="purposeQ" value="Query">
												</div>
											</div>
										</div>
									</div>

                                    <button id="queries_button" class="btn btn-primary card-1">SUBMIT &nbsp <span class="lodingLogin"> <img src="/images/comment-ajax.gif"  /></span></button>
                                </form>
                            </div>
						<div id="contact_hide">
							<span style="margin-top:1em;" class="icon"><i class="icon-shopping-cart"></i></span>
							<h2>Thank you for visiting. Our team contact you as soon as possible.</h2>
							<p>
								<a href="/"class="btn btn-primary">Home</a>
								<a href="/buy-cars"class="btn btn-primary btn-outline">Continue Shopping</a>
							</p>
						</div>
						
					</div>
				</div>
			</div>
		</div>

        <% include ../partials/footer %>