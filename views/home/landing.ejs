            </div>
        </div>
        <div class="row">

            <% data.forEach(function(cars){ %>
                <div class="col-md-3 text-center">
                    <div class="product-entry">
                        <div class="product-img" style="background-image: url(<%=cars.carImage1%>);">
                            <p class="tag"><span class="new">New</span></p>
                            <div class="cart">
                                <p>
                                    <span><a href="/view_details/<%=cars.id%>"><i class="icon-eye"></i></a></span> 
                                </p>
                            </div>
                        </div>
                        <div class="desc">
                            <h3><a><%=cars.model%></a></h3>
                            <p class="price"><span>Rs <%=cars.price%></span></p>
                        </div>
                    </div>
                </div>
            <%});%>
        </div>
    </div>
</div>

