                <div id="colorlib-contact">
                    <div class="container">
                        <div class="row">

                            <div class="col-md-10 col-md-offset-1">
                                <div style="background-color:#FFC300; " class="card card-2">
                                    <div style="padding:1em;" class="card-body">
                                        <h3 style="text-align:center; color:white;">Sell Your Car Online</h3>
                                    </div>
                                </div>
                                
                            </div>
                            
                            <div style="margin-top:1em;" class="col-md-10 col-md-offset-1">
                                <div class="contact-wrap">
                                    <h3>SELL YOUR CAR</h3>
                                    <form action="/admin/add_new_car" method="post" enctype="multipart/form-data">

                                        <div style="padding-top:1em;" class="form-row">
                                            <div class="form-group col-md-4">
                                                <div class="form-group">
                                                    <label for="location_1">Location 1</label>
                                                    <select name="location1" class="form-control" id="location_1" required>
                                                        <option disabled selected>Select</option>
                                                        <option>Delhi</option>
                                                        <option>Punjab</option>
                                                        <option>Gujarat</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <div class="form-group">
                                                    <label for="location_2">Location 2</label>
                
                                                    <select name="location2" class="form-control" id="location_2" required>
                                                        <option disabled selected>Select</option>
                                                        <option>Delhi</option>
                                                        <option>Punjab</option>
                                                        <option>Gujarat</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <div class="form-group">
                                                    <label for="location_3">location 3</label>
                                                    <select name="location3" class="form-control" id="location_3" required>
                                                        <option disabled selected>Select</option>
                                                        <option>Delhi</option>
                                                        <option>Punjab</option>
                                                        <option>Gujarat</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                
                                        <div class="form-row">
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="category">Select make</label>
                                                    <select name="catergory" onchange="selectCategorytrim(this.value)" id="allCategortrimAdd" class="form-control" required>
                
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="make">Select model</label>
                                                    <select name="make" onchange="selectMaketrim(this.value)" id="allMaketrim" class="form-control" required>
                                                        <option>Select category</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                
                                        <div class="form-row">
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="model">Select submodel</label>
                                                    <select name="model" onchange="selectModeltrim(this.value)" id="allMadetrim" class="form-control" required>
                                                        <option>Select make</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="year">Select year</label>
                                                    <select onchange="selectYeartrim(this.value)" name="year" id="allModeltrim" class="form-control" required>
                                                        <option>Select model</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                
                                        <div class="form-row">
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="trim">Select trim</label>
                                                    <select name="trim" id="allYeartrim" class="form-control" required>
                                                        <option>Select year</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="fristname">Owner name</label>
                                                    <input type="text" name="owner" class="form-control" id="fristname" placeholder="Owner name" required>
                                                </div>
                                            </div>
                                        </div>
                
                                        <div class="form-row">
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="kilometersDriven">Kilometers Driven</label>
                                                    <input type="text" name="kilometersDriven" class="form-control" id="kilometersDriven" placeholder="KilometersDriven" required>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="vehicleRegNo">Vehicle RegNo</label>
                                                    <input type="text" name="vehicleRegNo" class="form-control" id="vehicleRegNo" placeholder="VehicleRegNo" required>
                                                </div>
                                            </div>
                                        </div>
                
                                        <div class="form-row">
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="carPic">Upload car pics</label>
                                                    <input style="padding:1em;" id="carPic" name="myImage" type="file" class="form-control-file" multiple>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <div class="form-group">
                                                    <label for="price">Price</label>
                                                    <input type="text" name="price" class="form-control" id="price" placeholder="Price" required>
                                                </div>
                                            </div>
                                        </div>
                                        <button type="submit" class="btn btn-primary card-1">SUBMIT</button>
                                        
                                    </form>

                                    <!-- <div style="background-color:#cce5ff; margin-top:1em;" class="card card-2">
                                        <div style="padding:1em;" class="card-body">
                                            <h3 style="text-align:center; color:#004085">Contact Information</h3>
                                        </div>
                                    </div> -->

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
