<% include ../partials/header %>
	<div id="page">
		<nav class="colorlib-nav" role="navigation">
			<div class="top-menu">
				<div class="container">
					<div class="row">
						<div class="col-xs-2">
                            <div id="colorlib-logo"><a href="/">Store</a></div>
                        </div>
                        <div class="col-xs-10 text-right menu-1">
                            <ul class="captial">
                                <%if(currentUser){%>
                                    <li class="active"><a href="/admin/view_all_cars">Admin</a></li>
                                <%}%>
                                <li ><a href="/">Home</a></li>
                                <li><a href="/buy-cars">Buy Cars</a></li>
                                <li><a href="/sell-cars">Sell Cars</a></li>
                                <li><a href="/contact-us">Get In Touch</a></li>
                                <%if(currentUser){%>
                                    <li><a href="/index/logout">Logout</a></li>
                                <%}%>
                            </ul>
                        </div>
					</div>
				</div>
			</div>
		</nav>
		<aside id="colorlib-hero" class="breadcrumbs">
			<div class="flexslider">
				<ul class="slides">
			   	<li style="background-image: url(/images/cover-img-1.jpg);">
			   		<div class="overlay"></div>
			   		<div class="container-fluid">
			   			<div class="row">
				   			<div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
				   				<div class="slider-text-inner text-center">
				   					<h1>Contact Seller</h1>
				   					
				   				</div>
				   			</div>
				   		</div>
			   		</div>
			   	</li>
			  	</ul>
		  	</div>
		</aside>

		<div class="colorlib-shop">
			<div class="container">
				<div class="row row-pb-lg">
					<div class="col-md-10 col-md-offset-1">
						<div class="process-wrap">
							<div class="process text-center active">
								<p><span>01</span></p>
								<h3>Product Page</h3>
							</div>
							<div class="process text-center active">
								<p><span>02</span></p>
								<h3>View Details</h3>
							</div>
							<div class="process text-center">
								<p><span>03</span></p>
								<h3>Contact Seller</h3>
							</div>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-md-10 col-md-offset-1 text-center">

                            <div class="card card-2">
                                    <table class="table table-borderless">
                                        <tbody>
                                          <tr>
                                            <th scope="row">Name</th>
                                            <td><%=data.firstName%> <%=data.lastName%></td>
                                          </tr>
                                          <tr>
                                            <th scope="row">Email Id</th>
                                            <td><%=data.emailId%></td>
                                          </tr>
                                          <tr>
                                            <th scope="row">Phone No</th>
                                            <td><%=data.phoneNo%></td>
                                          </tr>
                                          <tr>
                                            <th scope="row">Pin Code</th>
                                            <td><%=data.pincode%></td>
                                          </tr>
                                        </tbody>
                                    </table>
                            </div>

						<span class="icon"><i class="icon-shopping-cart"></i></span>
						<h2>Thank you for visiting</h2>
						<p>
							<a href="/"class="btn btn-primary">Home</a>
							<a href="shop.html"class="btn btn-primary btn-outline">Continue Shopping</a>
						</p>
					</div>
				</div>
			</div>
		</div>

        <% include ../partials/footer %>